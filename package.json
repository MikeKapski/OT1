{
  "name": "otus-learning",
  "version": "1.0.0",
  "description": "otus learning trainings",
  "main": "index.tsx",
  "dependencies": {
    "@types/react-dom": "^16.9.6",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "loki": "^0.20.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loki": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.18",
    "@types/jest": "^25.2.1",
    "@types/storybook__addon-info": "^5.2.1",
    "@types/storybook__addon-knobs": "^5.2.1",
    "@types/storybook__react": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "html-webpack-plugin": "^4.2.0",
    "jest": "^25.4.0",
    "prettier": "2.0.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-blocks": "^2.0.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "start": "npx webpack-dev-server --mode development --open --hot",
    "build": "npx webpack --mode production",
    "eslint": "node_modules/.bin/eslint .src/",
    "loki:update": "npm run build-storybook && npx loki update --requireReference --reactUri file:./storybook-static",
    "loki:approve": "npx loki approve",
    "test": "jest",
    "test:loki": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikeKapski/OT1.git"
  },
  "keywords": [
    "otus",
    "learning",
    "react"
  ],
  "author": "Mikhail Kapski",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/MikeKapski/OT1/issues"
  },
  "homepage": "https://github.com/MikeKapski/OT1#readme"
}
